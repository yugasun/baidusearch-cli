{
  "name": "baidusearch-cli",
  "version": "0.1.0",
  "description": "A TypeScript library for searching Baidu (百度搜索) programmatically",
  "keywords": [
    "baidu",
    "search",
    "web-scraping",
    "typescript",
    "crawler",
    "百度",
    "搜索"
  ],
  "homepage": "https://github.com/yugasun/baidusearch-cli#readme",
  "bugs": {
    "url": "https://github.com/yugasun/baidusearch-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yugasun/baidusearch-cli.git"
  },
  "license": "MIT",
  "author": "yugasun",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "baidusearch": "dist/cli.js",
    "baidusearch-cli": "dist/cli.js"
  },
  "directories": {
    "example": "examples"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build && chmod +x dist/cli.js",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit --skipLibCheck",
    "lint": "echo 'Linting...' && exit 0",
    "lint:fix": "echo 'Linting fixed' && exit 0",
    "format": "echo 'Formatting...' && exit 0",
    "format:check": "echo 'Format check passed' && exit 0",
    "release": "echo 'Use GitHub Actions for releases' && exit 0",
    "release:minor": "echo 'Use GitHub Actions for releases' && exit 0",
    "release:major": "echo 'Use GitHub Actions for releases' && exit 0",
    "release:patch": "echo 'Use GitHub Actions for releases' && exit 0",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.24",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
